<?xml version='1.0' encoding='UTF-8'?>  

<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">  
  
 <hibernate-mapping>  
  <class name="com.model.Student" table="emp1000">  
    <id name="id" type="int">  
     <generator class="identity"></generator>  
    </id>  
            
    <property name="firstName" type="java.lang.String"></property>  
    <property name="lastName" type="java.lang.String"></property>  
            
  </class>  
            
 </hibernate-mapping>